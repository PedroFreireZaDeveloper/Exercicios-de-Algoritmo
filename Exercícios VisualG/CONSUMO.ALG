Algoritmo "consumo"

Var

  distancia: inteiro
  combustivelGasto, consumoMedio: real

Inicio

  escreva("Distância percorrida: ")
  leia(distancia)
  escreva("Combustível gasto: ")
  leia(combustivelGasto)
  consumoMedio <- distancia/combustivelGasto
  escreval("Consumo médio = ", consumoMedio:3:3)

Fimalgoritmo